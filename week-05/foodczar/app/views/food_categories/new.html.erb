<% if @food_category.errors %>
  <ul>
    <% @food_category.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <%end%>
  </ul>
<%end%>

<%= form_for @food_category do |f|%>
  <p>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :image_src %>
    <%= f.text_field :image_src %>
  </p>
  <%= f.submit %>
<%end%>
